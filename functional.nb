        RowBox[{
         RowBox[{"-", "5"}], "<", "z", "<", "5"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"mass", " ", "=", " ", 
     RowBox[{
      RowBox[{"NIntegrate", "[", 
       RowBox[{"300", ",", " ", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}], "\[Element]", 
         "boat"}]}], "]"}], "+", "5000"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"under", " ", "=", " ", 
     RowBox[{"RegionIntersection", "[", 
      RowBox[{"boat", ",", "water"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"disp", " ", "=", " ", 
