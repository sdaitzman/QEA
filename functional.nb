Notebook[{
Cell[BoxData[
 RowBox[{
  RowBox[{"momentarm", "[", "theta_", "]"}], ":=", 
  RowBox[{"(", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Returns", " ", "the", " ", "moment", " ", "of", " ", "the", " ", "boat", 
     "\[IndentingNewLine]", "theta_:", " ", "the", " ", "heel", " ", "angle", 
     " ", "in", " ", "degrees"}], " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"boat", " ", "=", " ", 
     RowBox[{"ImplicitRegion", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", "^", "2"}], "/", "2"}], "<", "y", "<", "2"}], " ", "&&",
         " ", 
        RowBox[{
         RowBox[{"-", "2"}], " ", "<", " ", "x", "<", "2"}], "&&", 
        RowBox[{"0", "<", "y", "<", "2"}], " ", "&&", " ", 
        RowBox[{
         RowBox[{"-", "5"}], " ", "<", " ", "z", " ", "<", "5"}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"water", " ", "=", " ", 
     RowBox[{"ImplicitRegion", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Cos", "[", "theta", "]"}], "*", "y"}], " ", "+", " ", 
          RowBox[{
           RowBox[{"Sin", "[", "theta", "]"}], "*", "x"}]}], "<", "d"}], "&&", 
        RowBox[{
         RowBox[{"-", "2"}], "<", "x", "<", "2"}], "&&", 
        RowBox[{"0", "<", "y", "<", "2"}], " ", "&&", " ", 
        RowBox[{
         RowBox[{"-", "5"}], "<", "z", "<", "5"}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"mass", " ", "=", " ", 
     RowBox[{
      RowBox[{"NIntegrate", "[", 
       RowBox[{"300", ",", " ", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}], "\[Element]", 
         "boat"}]}], "]"}], "+", "5000"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"under", " ", "=", " ", 
     RowBox[{"RegionIntersection", "[", 
      RowBox[{"boat", ",", "water"}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"disp", " ", "=", " ", 
     RowBox[{"Integrate", "[", 
      RowBox[{"1000", ",", " ", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "under"}], 
       ",", " ", 
       RowBox[{"Assumptions", "\[Rule]", 
        RowBox[{"{", 
         RowBox[{"0", "<", "d", "<", "2"}], "}"}]}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"maxdisp", " ", "=", " ", 
     RowBox[{"disp", "/.", 
      RowBox[{"{", 
       RowBox[{"d", "\[Rule]", "2"}], "}"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"waterline", " ", "=", 
     RowBox[{"NSolve", "[", 
      RowBox[{
       RowBox[{"disp", "\[Equal]", "mass"}], ",", "d", ",", "Reals"}], 
      "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"xcom", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"mass", " "}]], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "x"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"zcom", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"mass", " "}]], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "z"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"ycom", " ", "=", " ", 
     RowBox[{"N", "[", " ", 
      RowBox[{
       FractionBox["1", "mass"], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "y"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"com", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"xcom", ",", "ycom", ",", "zcom"}], "}"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"fgrav", " ", "=", " ", 
     RowBox[{"-", 
      RowBox[{"(", 
       RowBox[{"mass", " ", "*", " ", "9.8"}], ")"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"fbuoy", " ", "=", " ", 
     RowBox[{"-", "fgrav"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"displaced", " ", "=", " ", 
     RowBox[{"N", "[", 
      FractionBox["fgrav", 
       RowBox[{" ", 
        RowBox[{"1000", "*", "9.8"}]}]], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"draft", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{"d", " ", "/.", " ", 
       RowBox[{"waterline", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"cob", " ", "=", " ", 
     RowBox[{"RegionCentroid", "[", 
      RowBox[{"under", "/.", 
       RowBox[{"{", 
        RowBox[{"d", "\[Rule]", "draft"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"moment", " ", "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"cob", "-", "com"}], ")"}], "\[Cross]", 
      RowBox[{"{", 
       RowBox[{
        RowBox[{"fbuoy", "*", 
         RowBox[{"Sin", "[", "theta", "]"}]}], ",", 
        RowBox[{"fbuoy", "*", 
         RowBox[{"Cos", "[", "theta", "]"}]}], ",", "0"}], "}"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"moment", "[", 
     RowBox[{"[", "3", "]"}], "]"}]}], "\[IndentingNewLine]", 
   ")"}]}]], "Input",
