    RowBox[{"maxdisp", " ", "=", " ", 
     RowBox[{"disp", "/.", 
      RowBox[{"{", 
       RowBox[{"d", "\[Rule]", "2"}], "}"}]}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"waterline", " ", "=", 
     RowBox[{"NSolve", "[", 
      RowBox[{
       RowBox[{"disp", "\[Equal]", "mass"}], ",", "d", ",", "Reals"}], 
      "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"xcom", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"mass", " "}]], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "x"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"zcom", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{
       FractionBox["1", 
        RowBox[{"mass", " "}]], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "z"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"ycom", " ", "=", " ", 
     RowBox[{"N", "[", " ", 
      RowBox[{
       FractionBox["1", "mass"], "*", 
       RowBox[{"Integrate", "[", 
        RowBox[{
         RowBox[{"300", "*", "y"}], ",", " ", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"x", ",", "y", ",", "z"}], "}"}], "\[Element]", "boat"}]}],
         "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"com", " ", "=", " ", 
     RowBox[{"{", 
      RowBox[{"xcom", ",", "ycom", ",", "zcom"}], "}"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"fgrav", " ", "=", " ", 
     RowBox[{"-", 
      RowBox[{"(", 
       RowBox[{"mass", " ", "*", " ", "9.8"}], ")"}]}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"fbuoy", " ", "=", " ", 
     RowBox[{"-", "fgrav"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"displaced", " ", "=", " ", 
     RowBox[{"N", "[", 
      FractionBox["fgrav", 
       RowBox[{" ", 
        RowBox[{"1000", "*", "9.8"}]}]], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"draft", " ", "=", " ", 
     RowBox[{"N", "[", 
      RowBox[{"d", " ", "/.", " ", 
       RowBox[{"waterline", "[", 
        RowBox[{"[", "1", "]"}], "]"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
    RowBox[{"cob", " ", "=", " ", 
