      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"x", "^", "2"}], "/", "2"}], "<", "y", "<", "2"}], " ", "&&",
         " ", 
        RowBox[{
         RowBox[{"-", "2"}], " ", "<", " ", "x", "<", "2"}], "&&", 
        RowBox[{"0", "<", "y", "<", "2"}], " ", "&&", " ", 
        RowBox[{
         RowBox[{"-", "5"}], " ", "<", " ", "z", " ", "<", "5"}]}], ",", " ", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y", ",", " ", "z"}], "}"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"water", " ", "=", " ", 
     RowBox[{"ImplicitRegion", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Cos", "[", "theta", "]"}], "*", "y"}], " ", "+", " ", 
          RowBox[{
           RowBox[{"Sin", "[", "theta", "]"}], "*", "x"}]}], "<", "d"}], "&&", 
